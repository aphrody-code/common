          "children": []
        },
        {
          "name": "TEXT_INFO_17",
          "variables": [
            {
              "type": "Int",
              "value": "554110852"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Je vois. Cette visite l'autre jour était une sorte\\nd'inspection préalable à la fusion, alors ?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_18",
