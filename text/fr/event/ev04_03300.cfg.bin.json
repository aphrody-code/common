          "variables": [
            {
              "type": "Int",
              "value": "-1567208763"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Des techniques spéciales ?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_36",
          "variables": [
            {
              "type": "Int",
              "value": "-1148355708"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Tu veux qu'on apprenne des techniques spéciales avant le match ?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_37",
          "variables": [
            {
              "type": "Int",
              "value": "1008035659"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Oui. Nos attaquants, <FLC:SAKURAZAKI> et <FLC:RAIKA>\\nvont apprendre de puissantes techniques de tir qui pourront vaincre leur gardien."
            },
            {
              "type": "Int",
