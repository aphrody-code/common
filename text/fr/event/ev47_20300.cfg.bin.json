          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_7",
          "variables": [
            {
              "type": "Int",
              "value": "-321021865"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Ouais. C'était juste avant mes premiers examens.\\nJ'étais tellement stressé que j'ai fini par\\npleurer dans un coin.\\nOn m'a donné ce ballon pour me consoler."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_8",
          "variables": [
            {
              "type": "Int",
              "value": "-171529962"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Savoir qu'une seule personne me soutenait...\\nça comptait beaucoup pour moi."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_9",
