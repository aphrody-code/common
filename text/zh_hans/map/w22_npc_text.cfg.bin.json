            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_21",
          "variables": [
            {
              "type": "Int",
              "value": "-220814930"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "能感受到历史的积淀……"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_22",
          "variables": [
