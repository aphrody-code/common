            {
              "type": "Int",
              "value": "2049453439"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "就交给我校严谨负责的学生会来处理吧"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
