            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "真拿你没辙。"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_40",
