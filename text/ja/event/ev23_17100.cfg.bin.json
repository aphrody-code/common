            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_3",
          "variables": [
            {
              "type": "Int",
              "value": "146532188"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "ロココ…！\\n[俺/おれ]たちだって\u3000[負/ま]けないぜ！"
            },
            {
              "type": "Int",
              "value": "0"
            }
