          "children": []
        },
        {
          "name": "TEXT_INFO_4",
          "variables": [
            {
              "type": "Int",
              "value": "-523242194"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "やる[気/き]に[満/み]ちているようだな。"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_5",
