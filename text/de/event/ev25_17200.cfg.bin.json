            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Auch wenn Sie falsch liegen, können Sie jetzt nicht das Richtige tun?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_7",
          "variables": [
