          "children": []
        },
        {
          "name": "TEXT_INFO_6",
          "variables": [
            {
              "type": "Int",
              "value": "-1177827875"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Du kannst mich <FST:HARU> nennen.\\nWobei ich bezweifle, dass du mich anders nennen könntest."
            },
            {
              "type": "Int",
              "value": "0"
            }
