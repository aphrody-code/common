            {
              "type": "Int",
              "value": "605519316"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Hah ... Das ist ja mal ein interessanter Typ!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
