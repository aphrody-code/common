            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Käpt'n, bist du dir da sicher?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_16",
