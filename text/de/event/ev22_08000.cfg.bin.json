            {
              "type": "Int",
              "value": "1937369740"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Jetzt bist du dran, die Demütigung der Niederlage zu schmecken.\\nWir haben nämlich einen geheimen Plan ..."
            },
            {
              "type": "Int",
              "value": "0"
            }
