            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Wir haben beschlossen, uns dem Widerstand anzuschließen\\nund gegen Fifth Sector zu kämpfen."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
