            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_4",
          "variables": [
            {
              "type": "Int",
              "value": "-1672891500"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Mucha gente ha dejado de usarlos\\nal darse cuenta de la cantidad de energía que gastan."
