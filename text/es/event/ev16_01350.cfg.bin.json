            {
              "type": "Int",
              "value": "2"
            },
            {
              "type": "String",
              "value": "¡Si llega hasta mí, lo pararé sí o sí!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
