            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_28",
          "variables": [
            {
              "type": "Int",
              "value": "-15641433"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "¡¿Los Devoramentes?!"
