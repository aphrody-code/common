            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_37",
          "variables": [
            {
              "type": "Int",
              "value": "1615860510"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "¡Crear el keshin definitivo para acabar con los\\nlos Devoramentes!"
