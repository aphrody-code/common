            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_33",
          "variables": [
            {
              "type": "Int",
              "value": "489154395"
            },
            {
              "type": "Int",
              "value": "0"
            },
