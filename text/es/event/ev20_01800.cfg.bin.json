          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_6",
          "variables": [
            {
              "type": "Int",
              "value": "2064489478"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Quizás también podríamos depositar nuestras\\nesperanzas en vosotros..."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_7",
          "variables": [
            {
              "type": "Int",
              "value": "-57311031"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "¿Cómo?..."
