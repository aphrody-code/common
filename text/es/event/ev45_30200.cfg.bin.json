            {
              "type": "Int",
              "value": "253093476"
            },
            {
              "type": "Int",
              "value": "2"
            },
            {
              "type": "String",
              "value": "¡Seguro que es uno de los malvados planes de la Sociedad Oscura!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_4",
