            {
              "type": "Int",
              "value": "2083034846"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "... Vale, entendido."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_8",
