            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_14",
          "variables": [
            {
              "type": "Int",
              "value": "816467741"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Bueno, pues comencemos nuestra reunión secreta."
