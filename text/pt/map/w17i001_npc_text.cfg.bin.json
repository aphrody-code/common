            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_19",
          "variables": [
            {
              "type": "Int",
              "value": "352534263"
            },
            {
              "type": "Int",
              "value": "2"
            },
            {
              "type": "String",
              "value": "E é muito gostoso de comer."
