            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_24",
          "variables": [
            {
              "type": "Int",
              "value": "1644432993"
            },
            {
              "type": "Int",
              "value": "2"
            },
            {
              "type": "String",
              "value": "Sim, bem leve.\\nUma delícia."
