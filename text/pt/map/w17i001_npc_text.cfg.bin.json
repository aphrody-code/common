            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_17",
          "variables": [
            {
              "type": "Int",
              "value": "352534263"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Dá para mastigar bem\\nesse macarrão..."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_18",
          "variables": [
            {
              "type": "Int",
              "value": "352534263"
            },
            {
              "type": "Int",
              "value": "1"
            },
            {
              "type": "String",
              "value": "Sim, a textura é perfeita."
