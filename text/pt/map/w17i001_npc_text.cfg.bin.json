            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_15",
          "variables": [
            {
              "type": "Int",
              "value": "-1945473203"
            },
            {
              "type": "Int",
              "value": "3"
            },
            {
              "type": "String",
              "value": "Não sabia disso."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_16",
          "variables": [
            {
              "type": "Int",
              "value": "-2067993971"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "O caldo é feito com peixe voador. Não sabia disso, né?"
