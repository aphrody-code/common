            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_20",
          "variables": [
            {
              "type": "Int",
              "value": "352534263"
            },
            {
              "type": "Int",
              "value": "3"
            },
            {
              "type": "String",
              "value": "O caldo também\\nnão fica para trás."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_21",
          "variables": [
            {
              "type": "Int",
              "value": "-1349404338"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "A <FST:TSUKIMI> faz o udon. Acho que ela se inspira\\nno macarrão servido nas ilhas de Nagasaki."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_22",
          "variables": [
            {
              "type": "Int",
              "value": "1644432993"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Fukuoka..."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_23",
          "variables": [
            {
              "type": "Int",
              "value": "1644432993"
            },
            {
              "type": "Int",
              "value": "1"
            },
            {
              "type": "String",
              "value": "Não é conhecida por\\nmacarrão suave?"
