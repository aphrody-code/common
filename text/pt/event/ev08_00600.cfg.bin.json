            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_13",
          "variables": [
            {
              "type": "Int",
              "value": "-803722369"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "O quê?"
