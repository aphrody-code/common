            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_33",
          "variables": [
            {
              "type": "Int",
              "value": "-780934222"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Você não vai conseguir se safar disso!"
