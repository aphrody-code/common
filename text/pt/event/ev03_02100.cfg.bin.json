        },
        {
          "name": "TEXT_INFO_37",
          "variables": [
            {
              "type": "Int",
              "value": "2110172350"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Você está demitido. Saia da minha frente agora."
            },
            {
              "type": "Int",
