            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_32",
          "variables": [
            {
              "type": "Int",
              "value": "-932654349"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Diretor, essas crianças estão falando besteira!\\nIsso não é verdade!"
