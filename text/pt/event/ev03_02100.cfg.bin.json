            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_6",
          "variables": [
            {
              "type": "Int",
              "value": "-519880855"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Quem armou o incidente todo cinco anos atrás...\\nfoi você!"
