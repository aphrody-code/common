            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_27",
          "variables": [
            {
              "type": "Int",
              "value": "591507933"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Nosso antigo técnico, <FUL:SHIMOZURU>,\\nvai assumir o posto como novo técnico do <MNT:NAGUMOHARA>."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_28",
          "variables": [
            {
              "type": "Int",
              "value": "979009692"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "?!"
