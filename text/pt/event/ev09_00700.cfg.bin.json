            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_19",
          "variables": [
            {
              "type": "Int",
              "value": "-834186230"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Um bom técnico multiplica a força do time."
