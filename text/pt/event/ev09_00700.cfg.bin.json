            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_15",
          "variables": [
            {
              "type": "Int",
              "value": "-1705292331"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Acho que podemos ir bem, mesmo sem técnico..."
