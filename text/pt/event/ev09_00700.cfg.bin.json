            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_14",
          "variables": [
            {
              "type": "Int",
              "value": "-1317659114"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Mas ele nem dava muito direcionamento durante as partidas mesmo."
