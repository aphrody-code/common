            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_26",
          "variables": [
            {
              "type": "Int",
              "value": "-1169223245"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Uma gerente consegue fazer tanta diferença assim?"
