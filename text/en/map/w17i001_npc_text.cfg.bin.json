            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_38",
          "variables": [
            {
              "type": "Int",
              "value": "309263086"
            },
            {
              "type": "Int",
              "value": "1"
            },
            {
              "type": "String",
              "value": "Only if they win?"
            },
            {
              "type": "Int",
              "value": "0"
