            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_34",
          "variables": [
            {
              "type": "Int",
              "value": "-1956140204"
            },
            {
              "type": "Int",
              "value": "2"
            },
            {
              "type": "String",
              "value": "Think they've\\ngot noodles?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_35",
          "variables": [
            {
              "type": "Int",
              "value": "-1956140204"
            },
            {
              "type": "Int",
              "value": "3"
            },
            {
              "type": "String",
              "value": "No clue."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_36",
          "variables": [
            {
              "type": "Int",
              "value": "-523227255"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "I mean... if they're selling noodles there,\\nmaybe say I'm with your team or something? Help a guy out."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_37",
          "variables": [
            {
              "type": "Int",
              "value": "309263086"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Limited-edition menu?"
