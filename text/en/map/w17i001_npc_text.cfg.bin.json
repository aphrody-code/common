          "children": []
        },
        {
          "name": "TEXT_INFO_39",
          "variables": [
            {
              "type": "Int",
              "value": "309263086"
            },
            {
              "type": "Int",
              "value": "2"
            },
            {
              "type": "String",
              "value": "Guess we better root\\nfor 'em, then!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_40",
          "variables": [
            {
              "type": "Int",
              "value": "309263086"
            },
            {
              "type": "Int",
              "value": "3"
            },
            {
              "type": "String",
              "value": "Real subtle..."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_41",
          "variables": [
            {
              "type": "Int",
              "value": "-872580022"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "If your team makes the finals, they'll roll out a special noodle bowl.\\nPlease—beat Royal Academy! Do it for me. And the noodles."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_42",
          "variables": [
            {
              "type": "Int",
              "value": "-2100172929"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Cutlet topping, huh?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_43",
          "variables": [
            {
              "type": "Int",
              "value": "-2100172929"
            },
            {
              "type": "Int",
              "value": "1"
            },
            {
              "type": "String",
              "value": "For luck, I bet."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_44",
          "variables": [
            {
              "type": "Int",
              "value": "-2100172929"
            },
            {
              "type": "Int",
              "value": "2"
            },
            {
              "type": "String",
              "value": "No way I'm missing that."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_45",
          "variables": [
            {
              "type": "Int",
              "value": "-2100172929"
            },
            {
              "type": "Int",
              "value": "3"
            },
            {
              "type": "String",
              "value": "Man, you're way\\ntoo into this."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_46",
