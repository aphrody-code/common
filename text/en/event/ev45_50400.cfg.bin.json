            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_6",
          "variables": [
            {
              "type": "Int",
              "value": "-1752488238"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "No one understands him better than you do."
