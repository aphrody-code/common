            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "By believing in me until the end, you and <FUL:ARTHUR> have\\ndispelled the darkness that clouded my heart."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
