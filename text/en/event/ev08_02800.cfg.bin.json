      "children": [
        {
          "name": "TEXT_INFO_0",
          "variables": [
            {
              "type": "Int",
              "value": "316670100"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Raimon Junior High's Captain... <FUL:REN>, huh...?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_1",
          "variables": [
            {
              "type": "Int",
              "value": "969757527"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Yep!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_2",
          "variables": [
            {
              "type": "Int",
              "value": "550920726"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "It's a real honor to meet you, sir! I'm a huge fan!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_3",
          "variables": [
            {
              "type": "Int",
              "value": "1872229585"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Fan...? Hahaha!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_4",
          "variables": [
            {
              "type": "Int",
              "value": "1988937104"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "I gotta say, Raimon's in much better shape now\\nthan when I was on the team."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_5",
          "variables": [
            {
              "type": "Int",
              "value": "1570870867"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "But, the current Raimon wouldn't be here"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_6",
          "variables": [
            {
              "type": "Int",
              "value": "1153083154"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "if it... weren't for you, right?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_7",
          "variables": [
            {
              "type": "Int",
              "value": "-1021133859"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Sorry."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_8",
          "variables": [
            {
              "type": "Int",
              "value": "-633763172"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Loosen up, will ya?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_9",
          "variables": [
            {
              "type": "Int",
              "value": "171531234"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "You came here because you're worried about <FST:HARU>, huh?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_10",
          "variables": [
            {
              "type": "Int",
              "value": "321022627"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Well, yes."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_11",
          "variables": [
            {
              "type": "Int",
              "value": "940521824"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "He hasn't been showing up to the club,\\nand I heard he's not attending school either..."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_12",
          "variables": [
            {
              "type": "Int",
              "value": "554960929"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Could it be... Are the rumors true?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_13",
          "variables": [
            {
              "type": "Int",
              "value": "1851104998"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "The rumors?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_14",
          "variables": [
            {
              "type": "Int",
              "value": "2001645479"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "...About him having no hope of recovery..."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_15",
          "variables": [
            {
              "type": "Int",
              "value": "1550056548"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Is that what they say?\\nWell, it's possible."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_16",
          "variables": [
            {
              "type": "Int",
              "value": "1165544741"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "It's all up to <FST:HARU>. I don't believe anyone's beyond recovery.\\nThere's always a way to go back to the world you love."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_17",
          "variables": [
            {
              "type": "Int",
              "value": "-1025453590"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "The only person stopping you is yourself.\\nBut it seems <FST:HARU> still hasn't found his own answer yet."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_18",
          "variables": [
            {
              "type": "Int",
              "value": "-604249941"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "So that means\\nhe won't be able to play football anymore, right?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_19",
          "variables": [
            {
              "type": "Int",
              "value": "142599611"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "The reason <FST:HARU> hasn't come back\\nisn't just because of his leg injury."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_20",
          "variables": [
            {
              "type": "Int",
              "value": "291820794"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Huh?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_21",
          "variables": [
            {
              "type": "Int",
              "value": "977897273"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Think about it! Would a busted leg stop you from\\nplaying the way you want if you really loved football that much?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_22",
          "variables": [
            {
              "type": "Int",
              "value": "592623224"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "You've seen some, right?\\nOpponents who're like that."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_23",
          "variables": [
            {
              "type": "Int",
              "value": "1813192895"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "You mean the one from <MNT:NAGUMOHARA>, <FUL:UNMEI>?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_24",
          "variables": [
            {
              "type": "Int",
              "value": "1963463166"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Yeah, I guess."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_25",
          "variables": [
            {
              "type": "Int",
              "value": "1579500093"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "<FUL:REN>!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_26",
          "variables": [
            {
              "type": "Int",
              "value": "1195275132"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Yes!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_27",
          "variables": [
            {
              "type": "Int",
              "value": "-1062834253"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "For now, just believe in <FST:HARU> and wait for him.\\nKnowing him as I do, I'm sure he'll find his answer."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_28",
          "variables": [
            {
              "type": "Int",
              "value": "-641900814"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Mr. <LST:ENDO>..."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_29",
          "variables": [
            {
              "type": "Int",
              "value": "163417996"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "You all were overestimating <FST:HARU>\\nwhen you called him a \"Monster...\""
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_30",
          "variables": [
            {
              "type": "Int",
              "value": "279363277"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "The real monster, I would say..."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_31",
          "variables": [
            {
              "type": "Int",
              "value": "999025934"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "...is that <FUL:UNMEI>.\\nKid's tough, stay sharp."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_32",
          "variables": [
            {
              "type": "Int",
              "value": "579918927"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "I do want Raimon to win, after all."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_33",
          "variables": [
            {
              "type": "Int",
              "value": "1842432648"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Yes! Raimon's gonna win no matter what!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_34",
          "variables": [
            {
              "type": "Int",
              "value": "1959427017"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Right!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_35",
          "variables": [
            {
              "type": "Int",
              "value": "1608984586"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Well... Now, if you'll excuse me!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_36",
          "variables": [
            {
              "type": "Int",
              "value": "1190926667"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Hold on, <FUL:REN>."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_37",
          "variables": [
            {
              "type": "Int",
              "value": "-1050409596"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Huh?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_38",
          "variables": [
            {
              "type": "Int",
              "value": "-662752059"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Stay a little while and have dinner before you go."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        }
      ]
    }
  ]
