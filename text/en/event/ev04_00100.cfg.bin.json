            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_4",
          "variables": [
            {
              "type": "Int",
              "value": "-1542468498"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Things didn't go entirely according to plan, though."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_5",
          "variables": [
            {
              "type": "Int",
              "value": "-1893556307"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "And you have yet to submit an official club application."
