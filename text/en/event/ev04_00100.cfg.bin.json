            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_6",
          "variables": [
            {
              "type": "Int",
              "value": "-1774604564"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "What? Really?"
