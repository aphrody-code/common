            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_6",
          "variables": [
            {
              "type": "Int",
              "value": "1918599341"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Ugh! Stupid gorilla! Stupid bowl!\\nThose stickers should've been burned and erased from history!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_7",
          "variables": [
            {
              "type": "Int",
              "value": "-171729822"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "That drawing... and your design work..."
