            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_5",
          "variables": [
            {
              "type": "Int",
              "value": "1799401964"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "That's mean, Dad!"
