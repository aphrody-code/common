            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_32",
          "variables": [
            {
              "type": "Int",
              "value": "-925853917"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Mamma, che vuoi dire...?"
