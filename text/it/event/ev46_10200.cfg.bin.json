            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_29",
          "variables": [
            {
              "type": "Int",
              "value": "-469908256"
            },
            {
              "type": "Int",
              "value": "0"
