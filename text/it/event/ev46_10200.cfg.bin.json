            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_27",
          "variables": [
            {
              "type": "Int",
              "value": "719725791"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Hai sentito, mamma? Ha detto che è buono!"
