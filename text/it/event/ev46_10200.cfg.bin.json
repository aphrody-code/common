            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_21",
          "variables": [
            {
              "type": "Int",
              "value": "-804664235"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "<FST:UNMEI>... Bene."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_22",
          "variables": [
            {
              "type": "Int",
              "value": "-921502444"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Bene, <FST:FUMIO>!\\nSe davvero hai fatto pratica, dimostracelo! Qui e ora!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_23",
          "variables": [
            {
              "type": "Int",
              "value": "-2041353261"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Adesso vedrete!"
