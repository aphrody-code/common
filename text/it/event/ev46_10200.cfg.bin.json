          "variables": [
            {
              "type": "Int",
              "value": "-883670195"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Mi sono allenato duramente nell'arte della creazione degli udon! Davvero!"
