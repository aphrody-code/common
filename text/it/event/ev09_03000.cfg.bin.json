      "children": [
        {
          "name": "TEXT_INFO_0",
          "variables": [
            {
              "type": "Int",
              "value": "-1971489194"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Che male c'è a fallire?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_1",
          "variables": [
            {
              "type": "Int",
              "value": "-1588583019"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Basterà rialzarsi e riprovarci.\\nTutte le volte che serve."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_2",
          "variables": [
            {
              "type": "Int",
              "value": "-1203039020"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Volta dopo volta, a costo di sembrare patetici.\\nL'importante è non mollare mai!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_3",
          "variables": [
            {
              "type": "Int",
              "value": "-150306285"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Dimmi, c'è forse qualcosa di più bello?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_4",
          "variables": [
            {
              "type": "Int",
              "value": "-300829870"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "<FST:HARU>..."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_5",
          "variables": [
            {
              "type": "Int",
              "value": "-985866095"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Ero bloccato sull'Isola Awaji,\\na combattere contro la mia gamba che non voleva muoversi."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_6",
          "variables": [
            {
              "type": "Int",
              "value": "-601370160"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Altro che riabilitazione, sembrava un campo di addestramento."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_7",
          "variables": [
            {
              "type": "Int",
              "value": "1539296543"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "E quando mi sono guardato allo specchio...\\nTutto arruffato e disperato..."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_8",
          "variables": [
            {
              "type": "Int",
              "value": "1118108766"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Non ho mai visto qualcuno di così ridicolo."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_9",
          "variables": [
            {
              "type": "Int",
              "value": "-1834734304"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Ma per la prima volta..."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_10",
          "variables": [
            {
              "type": "Int",
              "value": "-1950409631"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Il calcio non mi è sembrato noioso.\\nMi è sembrato fantastico."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_11",
          "variables": [
            {
              "type": "Int",
              "value": "-1601024094"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Si è riaccesa in me una scintilla!\\nÈ in quel momento che ho deciso: tornerò a giocare, costi quel che costi."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_12",
          "variables": [
            {
              "type": "Int",
              "value": "-1182171421"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Eccolo! Sottovalutare <FUL:UNMEI> è un grave errore."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_13",
          "variables": [
            {
              "type": "Int",
              "value": "-154605532"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Non c'è dubbio, hanno già analizzato\\ntutte le nostre solite strategie."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_14",
          "variables": [
            {
              "type": "Int",
              "value": "-271328923"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Ecco perché... Se vogliamo batterlo,\\nl'unico modo è improvvisare!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_15",
          "variables": [
            {
              "type": "Int",
              "value": "-989951322"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Volete ancora...\\n...continuare a difendere i colori dell'invincibile Raimon?"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_16",
          "variables": [
            {
              "type": "Int",
              "value": "-572146713"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "La vita è una continua improvvisazione!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_17",
          "variables": [
            {
              "type": "Int",
              "value": "1518184232"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "E se si fallisce, non resta che riprovare."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_18",
          "variables": [
            {
              "type": "Int",
              "value": "1130796649"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Questo è il motivo per cui ho deciso di...\\n...giocare di nuovo a calcio!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        }
      ]
    }
