            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_11",
          "variables": [
            {
              "type": "Int",
              "value": "-1890078022"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Ah... sì."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_12",
          "variables": [
            {
              "type": "Int",
              "value": "-1773370373"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Ora capisco...\\ncosa si prova a non poter giocare."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_13",
          "variables": [
            {
              "type": "Int",
              "value": "-653454020"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Pensavo che il calcio non significasse nulla.\\nChe fosse persino noioso."
