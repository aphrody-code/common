            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_14",
          "variables": [
            {
              "type": "Int",
              "value": "-1072290691"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Ma ora... è frustrante, mi divora dentro..."
