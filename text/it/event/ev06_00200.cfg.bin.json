            {
              "type": "Int",
              "value": "-663997600"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Beh, per oggi vi lascerò in pace."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_32",
          "variables": [
            {
              "type": "Int",
              "value": "-1049165279"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Non vedo l'ora di giocare insieme a tutti voi."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_33",
          "variables": [
            {
              "type": "Int",
              "value": "-1909025562"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Pff, ci stava proprio guardando dall'alto in basso."
