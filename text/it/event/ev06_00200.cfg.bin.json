            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_34",
          "variables": [
            {
              "type": "Int",
              "value": "-1758616153"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Persino dopo che li abbiamo battuti..."
