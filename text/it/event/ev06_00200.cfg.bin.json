        },
        {
          "name": "TEXT_INFO_35",
          "variables": [
            {
              "type": "Int",
              "value": "-1140787612"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Di questo passo si prenderanno il club.\\nParliamone meglio da un'altra parte."
