          "children": []
        },
        {
          "name": "TEXT_INFO_28",
          "variables": [
            {
              "type": "Int",
              "value": "1305331095"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Quindi, rilassati!\\nQuando ti capiterà di sbagliare, saremo al tuo fianco."
