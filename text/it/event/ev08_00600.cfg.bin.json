            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_6",
          "variables": [
            {
              "type": "Int",
              "value": "-84471157"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "È una citazione dell'Arte della Guerra di Sun Tzu.\\nIn sostanza, i nostri prossimi avversari sono veri maestri della tattica."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_7",
          "variables": [
            {
              "type": "Int",
              "value": "2104430148"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Senza dimenticare che il loro geniale allenatore, <FUL:ALICE>,\\nriesce ad adattarsi a qualsiasi situazione."
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
