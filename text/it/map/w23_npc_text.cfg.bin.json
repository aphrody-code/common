          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_32",
          "variables": [
            {
              "type": "Int",
              "value": "-1912795059"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "La responsabile del Mercatone G, Cinnamon, ha dei riflessi assurdi.\\nNel corso degli anni ha realizzato assist spettacolari!"
            },
            {
              "type": "Int",
              "value": "0"
            }
          ],
          "children": []
        },
        {
          "name": "TEXT_INFO_33",
          "variables": [
            {
              "type": "Int",
              "value": "611763996"
            },
            {
              "type": "Int",
              "value": "0"
            },
            {
              "type": "String",
              "value": "Che resistenza\\nincredibile."
